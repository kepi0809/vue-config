{
  "name": "example-repo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "env WEBPACK_DEV_SERVER=true vue-cli-service serve",
    "build": "./build.sh",
    "lint": "npm run-script lint:script && npm run-script lint:style",
    "build:dev": "vue-cli-service build --mode development",
    "build:watch": "vue-cli-service build --watch",
    "bundle-report": "vue-cli-service build --report-json && webpack-bundle-analyzer ../public/spa/report.json",
    "count:translations": "node ./src/lang/count",
    "lint:fix": "npm run-script lint:script:fix && npm run-script lint:style:fix",
    "lint:script": "vue-cli-service lint --no-fix",
    "lint:script:fix": "vue-cli-service lint",
    "lint:style": "stylelint './src/**/*.vue' './src/**/*.scss'",
    "lint:style:fix": "stylelint './src/**/*.vue' './src/**/*.scss' --fix",
    "new": "HYGEN_TMPLS=generators hygen new --",
    "new:page": "npm run new page-with-table",
    "test": "jest",
    "test:unit": "jest --",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll"
  },
